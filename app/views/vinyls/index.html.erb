<%= render 'layouts/table' %>

<script type='text/javascript' charset='utf-8'>
$(function() {
    $.ajax({
        type: 'GET',
        url: '/vinyls.json'
    }).done(function(data) {
        console.log(data);
        div_html = '';
        $.each(data, function( index, value ) {
            div_html =  div_html + '<tr><th scope="row">'+ value['title'] +'</th></tr>';

        });

        console.log(div_html);
        $('#vinyls_table').html(div_html);
    });
});
</script>