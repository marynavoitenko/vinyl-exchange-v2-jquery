<%= render 'layouts/table' %>

<script type='text/javascript' charset='utf-8'>
$(function() {
    $.ajax({
        type: 'GET',
        url: '/vinyls.json'
    }).done(function(data) {
        console.log(data);
        div_html = '';
        $.each(data, function( index, value ) {
            div_html += '<tr>';
            div_html += '<th scope="row">' + value['artist_id'] +'</th>';
            div_html +=  '<th scope="row">'+ value['title'] +'</th>';
            div_html += '<th scope="row">' + value['price'] +'</th>';
            div_html += '<th scope="row">' + value['inventory'] +'</th>';
            div_html += '<th scope="row">' + value['for_sale'] +'</th>';
            div_html += '</tr>'
        });

        console.log(div_html);
        $('#vinyls_table').html(div_html);
    });
});
</script>